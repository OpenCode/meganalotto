PUBLIC $dati AS Float[]
PUBLIC $colore AS Integer
PUBLIC $titolo AS String

PUBLIC SUB _new(dati AS Float[], colore AS Integer, titolo AS String)
  
  $dati = dati.Copy()
  $colore = colore
  $titolo = titolo
  
END
